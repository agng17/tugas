#Volume kubus
print("menghitung volume kubus")
s = int(input("masukkan panjang rusuk kubus ="))
volumekubus = s * s * s
print("luas kubusnya adalah", volumekubus)

#Volume balok
print("menghitung volume balok")
p = int(input("masukkan panjang balok ="))
l = int(input("masukkan luas balok ="))
t = int(input("masukkan tinggi balok ="))
volumebalok = (p*l*t)
print("volume baloknya adalah", volumebalok)
